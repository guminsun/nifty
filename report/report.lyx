\begin_layout Subsubsection
Structure of the Translator
\end_layout

\begin_layout Standard
\begin_inset Wrap figure
lines 0
placement o
overhang 0in
width "40col%"
status open

\begin_layout Plain Layout
\paragraph_spacing single
